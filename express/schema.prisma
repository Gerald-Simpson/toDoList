datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model listTitle {
    id          Int         @id @default(autoincrement())
    cookieId    String 
    createdAt   DateTime    @default(now())
    title       String
}

model listItems {
    id          Int         @id @default(autoincrement())
    titleId     Int         
    createdAt   DateTime    @default(now())
    message     String
    complete    Boolean     @default(false)
}
